'.source.elm':
  'if':
    prefix: 'if'
      body: '''
        if ${1:expression} then
        \t$2
        else
        \t$3
      '''

  'if-elseif':
    prefix: 'ifelseif'
      body: '''
        if ${1:expression} then
        \t$2
        else if ${3:expression}
        \t$4
        else
        \t$5
      '''

  
